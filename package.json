{
  "name": "react-pagination-js",
  "version": "1.0.0",
  "description": "Simple, light weight pagination for react",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --output-filename index.umd.js --mode development",
    "build:umd.min": "webpack --output-filename index.umd.min.js --mode production",
    "start:dev": "webpack-dev-server",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaurssaurav/react-pagination-js.git"
  },
  "author": "isaurssaurav",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isaurssaurav/react-pagination-js/issues"
  },
  "homepage": "https://github.com/isaurssaurav/react-pagination-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.21",
    "validate-commit-msg": "^2.14.0",
    "cz-conventional-changelog": "^3.0.2"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
